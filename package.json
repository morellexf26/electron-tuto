{
  "name": "electron-tuto",
  "description": "The electron version of tuto is here. Start creating your macOS and Windows apps right now!",
  "private": false,
  "version": "1.1.1",
  "license": "MIT",
  "keywords": ["electron", "boilerplate", "vue", "vuex", "axios", "vite", "pinia"],
  "main": "main.js",
  "scripts": {
    "start": "vite build & electron .",
    "electron-packager-macos": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=./src/assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "electron-packager-win": "electron-packager . electron-tuto --overwrite --asar=true --platform=win32 --arch=ia32 --icon=./src/assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=tuto --version-string.FileDescription=tuto --version-string.ProductName='Electron Tuto'",
    "dev": "vite",
    "test": "jest",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "jest": "^27.0.0",
    "pinia": "^2.0.14",
    "pinia-plugin-persist": "^1.0.0",
    "vue": "^3.2.33",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.12",
    "@pinia/testing": "^0.0.12",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/cli-plugin-unit-jest": "^5.0.4",
    "@vue/test-utils": "^2.0.0-rc.21",
    "@vue/vue3-jest": "^27.0.0",
    "babel-jest": "^27.0.0",
    "babel-preset-vite": "^1.0.4",
    "electron": "^19.0.0",
    "electron-packager": "^15.5.1",
    "identity-obj-proxy": "^3.0.0",
    "sass": "^1.51.0",
    "vite": "^2.9.9",
    "vue-router-mock": "^0.1.7"
  }
}
